#!bin/picolisp lib.l
# 10jun08abu

(raw T)
(let *Dbg NIL
   (let? Sock (connect "localhost" (format (opt)))
      (out Sock
         (pr (list 'qsh (in ".pw" (line T)) (in '("tty") (line T))))
         (flush)
         (in Sock
            (ctty (rd))
            (line)
            (line) ) ) ) )
(bye)
