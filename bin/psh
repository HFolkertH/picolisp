(load "@lib/misc.l" "@lib/http.l")

(raw T)
(let *Dbg NIL
   (client "localhost" (format (car (argv)))
      (pack "@psh?" (pw) "&" (in '("tty") (line T)))
      (ctty (read))
      (line)
      (line) ) )
(bye)

