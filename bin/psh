(load "@lib/misc.l" "@lib/tmp.l" "@lib/rsa.l" "@lib/http.l")

(ctty)
(let *Dbg NIL
   (client "localhost" (format (car (argv)))
      (pack "@psh?" (pw) "&" (in '("tty") (line T)))
      (ctty (read))
      (line)
      (line) ) )
(bye)
